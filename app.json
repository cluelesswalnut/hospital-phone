{
  "expo": {
    "name": "hospital-phone",
    "slug": "hospital-phone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/osu_icon.png",
    "splash": {
      "image": "./assets/osu_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/osu_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.markusernst.ous_hospital_phone"
    },
    "web": {
      "favicon": "./assets/osu_icon.png"
    }
  }
}
